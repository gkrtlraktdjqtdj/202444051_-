#include <stdio.h>
#include <stdlib.h>

int main(void)
{
    system("title for 문으로 알파벳 대문자 출력");
    int i;
    printf("알파벳 대문자가 출력됩니다.\n");
    for(i=65; i <= 90; i++)
        printf("%c", i);
    return 0;
}
------------------------------------------------------별 쌓기
#include <stdio.h>
#include <stdlib.h>

int main(void)
{
  for(int i=1; i<=5; i++)
  {
    for(int j=1; j<=10; j++)
    {
        printf("*");
    }
    printf("\n");
  }
  
}
------------------------------------------------------
#include <stdio.h>
#include <stdlib.h>

int main(void)
{
  for(int i=5; i>=1; i--)
  {
    for(int j=1; j<=i; j++)
    {
        printf("*");
    }
    printf("\n");
  }
  
}
